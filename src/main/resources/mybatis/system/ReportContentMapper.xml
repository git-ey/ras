<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ReportContentMapper">
	
	<!--表名 -->
	<sql id="tableName">
		SYS_REPORT_CONTENT
	</sql>
	
	<!-- 字段 -->
	<sql id="Field">
		FUND_ID,	
		FS_ATTR1,	
		FS_ATTR2,	
		FS_ATTR3,	
		COMISSION_NOTE,	
		MF_ATTR1,	
		MF_ATTR2,	
		MF_ATTR3,	
		MF_ATTR4,	
		MF_ATTR5,	
		CF_ATTR1,	
		CF_ATTR2,	
		CF_ATTR3,	
		CF_ATTR4,	
		CF_ATTR5,
		SF_ATTR1,
		SF_ATTR2,
		SF_ATTR3,
		SF_ATTR4,
		SF_ATTR5,
		MR,
		IR,
		IR_EXP,
		IR_ATTR1,
		ER,	
		IR_UP,	
		IR_DOWN,	
		PR_UP,	
		PR_DOWN,	
		PR_ATTR1,	
		PR_ATTR2,	
		PR_ATTR3,	
		FV_ATTR1,	
		ACTIVE,	
		REPORTCONTENT_ID
	</sql>
	
	<!-- 字段值 -->
	<sql id="FieldValue">
		#{FUND_ID},	
		#{FS_ATTR1},	
		#{FS_ATTR2},	
		#{FS_ATTR3},	
		#{COMISSION_NOTE},	
		#{MF_ATTR1},	
		#{MF_ATTR2},	
		#{MF_ATTR3},	
		#{MF_ATTR4},	
		#{MF_ATTR5},	
		#{CF_ATTR1},	
		#{CF_ATTR2},	
		#{CF_ATTR3},	
		#{CF_ATTR4},	
		#{CF_ATTR5},	
		#{SF_ATTR1},
		#{SF_ATTR2},
		#{SF_ATTR3},
		#{SF_ATTR4},
		#{SF_ATTR5},
		#{MR},
		#{IR},
		#{IR_EXP},
		#{IR_ATTR1},
		#{ER},		
		#{IR_UP},	
		#{IR_DOWN},	
		#{PR_UP},	
		#{PR_DOWN},	
		#{PR_ATTR1},	
		#{PR_ATTR2},	
		#{PR_ATTR3},	
		#{FV_ATTR1},	
		#{ACTIVE},	
		#{REPORTCONTENT_ID}
	</sql>
	
	<!-- 新增-->
	<insert id="save" parameterType="pd">
		insert into 
	<include refid="tableName"></include>
		(
	<include refid="Field"></include>
		) values (
	<include refid="FieldValue"></include>
		)
	</insert>
	
	<!-- 删除-->
	<delete id="delete" parameterType="pd">
		delete from
		<include refid="tableName"></include>
		where 
			REPORTCONTENT_ID = #{REPORTCONTENT_ID}
	</delete>
	
	<!-- 修改 -->
	<update id="edit" parameterType="pd">
		update
		<include refid="tableName"></include>
		set 
			FUND_ID = #{FUND_ID},
			FS_ATTR1 = #{FS_ATTR1},
			FS_ATTR2 = #{FS_ATTR2},
			FS_ATTR3 = #{FS_ATTR3},
			COMISSION_NOTE = #{COMISSION_NOTE},
			MF_ATTR1 = #{MF_ATTR1},
			MF_ATTR2 = #{MF_ATTR2},
			MF_ATTR3 = #{MF_ATTR3},
			MF_ATTR4 = #{MF_ATTR4},
			MF_ATTR5 = #{MF_ATTR5},
			CF_ATTR1 = #{CF_ATTR1},
			CF_ATTR2 = #{CF_ATTR2},
			CF_ATTR3 = #{CF_ATTR3},
			CF_ATTR4 = #{CF_ATTR4},
			CF_ATTR5 = #{CF_ATTR5},
		    SF_ATTR1 = #{SF_ATTR1},
		    SF_ATTR2 = #{SF_ATTR2},
		    SF_ATTR3 = #{SF_ATTR3},
		    SF_ATTR4 = #{SF_ATTR4},
		    SF_ATTR5 = #{SF_ATTR5},
		    MR = #{MR},
		    IR = #{IR},
		    IR_EXP = #{IR_EXP},
		    IR_ATTR1 = #{IR_ATTR1},
		    ER = #{ER},
			IR_UP = #{IR_UP},
			IR_DOWN = #{IR_DOWN},
			PR_UP = #{PR_UP},
			PR_DOWN = #{PR_DOWN},
			PR_ATTR1 = #{PR_ATTR1},
			PR_ATTR2 = #{PR_ATTR2},
			PR_ATTR3 = #{PR_ATTR3},
			FV_ATTR1 = #{FV_ATTR1},
			ACTIVE = #{ACTIVE},
		REPORTCONTENT_ID = REPORTCONTENT_ID
		where 
		REPORTCONTENT_ID = #{REPORTCONTENT_ID}
	</update>
	
	<!-- 通过ID获取数据 -->
	<select id="findById" parameterType="pd" resultType="pd">
		select 
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 
			REPORTCONTENT_ID = #{REPORTCONTENT_ID}
	</select>
	
	<!-- 列表 -->
	<select id="datalistPage" parameterType="page" resultType="pd">
		select
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 1=1
		<if test="pd.keywords!= null and pd.keywords != ''"><!-- 关键词检索 -->
			and
				(
				<!-- 根据需求自己加检索条件 -->
					FUND_ID LIKE CONCAT(CONCAT('%', #{pd.keywords}),'%')
				)
		</if>
	</select>
	
	<!-- 列表(全部) -->
	<select id="listAll" parameterType="pd" resultType="pd">
		select
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
	</select>
	
	<!-- 批量删除 -->
	<delete id="deleteAll" parameterType="String">
		delete from
		<include refid="tableName"></include>
		where 
			REPORTCONTENT_ID in
		<foreach item="item" index="index" collection="array" open="(" separator="," close=")">
                 #{item}
		</foreach>
	</delete>
	
	<!-- 批量新增-->
	<insert id="saveBatch" parameterType="java.util.List">
		insert into 
	        <include refid="tableName"></include>
		(
	        <include refid="Field"></include>
		) values 
		<foreach collection ="list" item="pd" index= "index" separator =",">
         (
		#{pd.FUND_ID},	
		#{pd.FS_ATTR1},	
		#{pd.FS_ATTR2},	
		#{pd.FS_ATTR3},	
		#{pd.COMISSION_NOTE},	
		#{pd.MF_ATTR1},	
		#{pd.MF_ATTR2},	
		#{pd.MF_ATTR3},	
		#{pd.MF_ATTR4},	
		#{pd.MF_ATTR5},	
		#{pd.CF_ATTR1},	
		#{pd.CF_ATTR2},	
		#{pd.CF_ATTR3},	
		#{pd.CF_ATTR4},	
		#{pd.CF_ATTR5},	
		#{pd.SF_ATTR1},
		#{pd.SF_ATTR2},
		#{pd.SF_ATTR3},
		#{pd.SF_ATTR4},
		#{pd.SF_ATTR5},
		#{pd.MR},
		#{pd.IR},
		#{pd.IR_EXP},
		#{pd.IR_ATTR1},
		#{pd.ER},	
		#{pd.IR_UP},	
		#{pd.IR_DOWN},	
		#{pd.PR_UP},	
		#{pd.PR_DOWN},	
		#{pd.PR_ATTR1},	
		#{pd.PR_ATTR2},	
		#{pd.PR_ATTR3},	
		#{pd.FV_ATTR1},	
		#{pd.ACTIVE},	
		#{pd.REPORTCONTENT_ID}
         )
         </foreach >
	</insert>
	
</mapper>